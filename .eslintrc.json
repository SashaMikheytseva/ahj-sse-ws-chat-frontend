{
    "extends": "airbnb-base",
    "env": {
       "es6": true,
       "browser": true,
       "jest": true
    },
    "rules": {
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ], 
      "no-constructor-return": "off", 
      "import/no-extraneous-dependencies": "off", 
      "no-console": "off", 
      "class-methods-use-this": "off"

    }
  }